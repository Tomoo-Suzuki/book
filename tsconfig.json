{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "jsx": "react",
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "noImplicitAny": false,
        "alwaysStrict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true,
        "baseUrl": "./",
        "paths": {
            "_container*": ["src/container*"],
            "_components*": ["src/components*"],
            "_redux*": ["src/redux*"],
            "_img*": ["srcimg*"],
            "_scss*": ["src/scss*"],
            "_lib*": ["src/lib*"],
            "_queries*": ["src/queries*"]
        }
    }
}